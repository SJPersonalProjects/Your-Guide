<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/tour_guide_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tour_guide_app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.YourGuide"
        tools:targetApi="31">
        <activity
            android:name=".MapActivity"
            android:exported="false" />
        <activity
            android:name=".province_cards_categories.HistoricalPlaceActivity"
            android:exported="false" />
        <activity
            android:name=".province_cards_categories.TopRestaurantActivity"
            android:exported="false" />
        <activity
            android:name=".province_cards_categories.PopularCityActivity"
            android:exported="false" />
        <activity
            android:name=".kpk_province.KPKActivity"
            android:exported="false" />
        <activity
            android:name=".balochistan_province.BalochistanActivity"
            android:exported="false" />
        <activity
            android:name=".sindh_province.SindhActivity"
            android:exported="false" />
        <activity
            android:name=".punjab_province.PunjabActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Google maps meta-data-->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_api_key" />
    </application>

</manifest>